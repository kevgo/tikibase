{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "json": {},
  "markdown": {
    "textWrap": "always"
  },
  "toml": {},
  "includes": ["**/*.{json,md,toml}"],
  "excludes": [
    "target",
    "tmp"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.12.3.wasm",
    "https://plugins.dprint.dev/markdown-0.9.6.wasm",
    "https://plugins.dprint.dev/toml-0.5.0.wasm"
  ]
}
